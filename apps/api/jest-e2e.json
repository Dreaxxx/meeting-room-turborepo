{
  "preset": "ts-jest",
  "testEnvironment": "node",
  "rootDir": ".",
  "testMatch": [
    "<rootDir>/test/**/*.e2e-spec.ts"
  ],
  "moduleFileExtensions": [
    "ts",
    "js",
    "json"
  ],
  "setupFilesAfterEnv": [
    "<rootDir>/test/setup-e2e.ts"
  ],
  "globals": {
    "ts-jest": {
      "tsconfig": "<rootDir>/tsconfig.e2e.json"
    }
  }
}